{
  "name": "ts-babel",
  "description": "Transform TypeScript compiler result using Babel to granular target control",
  "version": "4.1.4",
  "license": "MIT",
  "author": "Vladimir Krivosheev",
  "bin": {
    "ts-babel": "out/builder.js"
  },
  "main": "out/builder.js",
  "files": [
    "out",
    "*.js",
    "tsconfig-base.json"
  ],
  "scripts": {
    "compile": "ts-babel",
    "release": "ts-babel && npm publish"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "bluebird-lst": "^1.0.3",
    "fs-extra-p": "^4.4.0",
    "source-map-support": "^0.4.16",
    "v8-compile-cache": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.25",
    "babel-preset-ts-node4-bluebird": "^0.1.0",
    "ts-babel": "^4.1.3",
    "typescript": "^2.4.2"
  },
  "peerDependencies": {
    "typescript": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ],
  "babel": {
    "presets": [
      "babel-preset-ts-node4-bluebird"
    ]
  }
}
