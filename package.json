{
  "name": "ts-babel",
  "description": "Transform TypeScript compiler result using Babel to granular target control",
  "version": "1.1.3",
  "license": "MIT",
  "bin": {
    "ts-babel": "main.js"
  },
  "files": [
    "out",
    "*.js"
  ],
  "scripts": {
    "compile": "tsc",
    "release": "tsc && npm publish"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "fs-extra-p": "^1.2.0",
    "markdown-it": "^8.0.1",
    "source-map-support": "^0.4.5"
  },
  "peerDependencies": {
    "typescript": "^2.1.0-dev.0 || ^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=0.4.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ],
  "devDependencies": {
    "@types/node": "^6.0.45"
  }
}
