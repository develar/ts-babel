{
  "name": "ts-babel",
  "description": "Transform TypeScript compiler result using Babel to granular target control",
  "version": "6.0.2",
  "license": "MIT",
  "author": "Vladimir Krivosheev",
  "bin": {
    "ts-babel": "out/builder.js"
  },
  "main": "out/builder.js",
  "files": [
    "out",
    "*.js",
    "tsconfig-base.json"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "release": "yarn compile && npm publish"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "bluebird-lst": "^1.0.5",
    "fs-extra-p": "^4.6.1",
    "source-map-support": "^0.5.9",
    "v8-compile-cache": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^10.9.2",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ],
  "babel": {
    "presets": [
      "babel-preset-ts-node6-bluebird"
    ]
  }
}
