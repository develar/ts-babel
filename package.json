{
  "name": "ts-babel",
  "description": "Transform TypeScript compiler result using Babel to granular target control",
  "version": "6.1.7",
  "license": "MIT",
  "author": "Vladimir Krivosheev",
  "bin": {
    "ts-babel": "out/builder.js"
  },
  "main": "out/builder.js",
  "files": [
    "out",
    "*.js",
    "tsconfig-base.json"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "release": "yarn compile && npm publish"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "bluebird": "^3.5.5",
    "fs-extra": "^8.1.0",
    "v8-compile-cache": "^2.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.0.10",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "typescript": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ]
}
