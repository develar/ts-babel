{
  "name": "ts-babel",
  "description": "Transform TypeScript compiler result using Babel to granular target control",
  "version": "1.3.4",
  "license": "MIT",
  "bin": {
    "ts-babel": "out/builder.js"
  },
  "main": "out/builder.js",
  "files": [
    "out",
    "*.js"
  ],
  "scripts": {
    "compile": "tsc",
    "release": "tsc && npm publish"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "bluebird-lst-c": "^1.0.5",
    "chalk": "^1.1.3",
    "fs-extra-p": "^3.0.3",
    "markdown-it": "^8.2.2",
    "source-map-support": "^0.4.8"
  },
  "peerDependencies": {
    "typescript": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=0.4.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ],
  "devDependencies": {
    "@types/node": "^7.0.0"
  }
}
