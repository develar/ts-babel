    "source-map-support": "^0.4.15",
    "v8-compile-cache": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.16"
  },
  "peerDependencies": {
    "typescript": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ]
