{
  "name": "ts-babel",
  "description": "Transform TypeScript compiler result using Babel to granular target control",
  "version": "1.4.3",
  "license": "MIT",
  "bin": {
    "ts-babel": "out/builder.js",
    "ts2jsdoc": "out/ts2jsdoc.js"
  },
  "main": "out/builder.js",
  "files": [
    "out",
    "*.js"
  ],
  "scripts": {
    "compile": "tsc",
    "release": "tsc && npm publish"
  },
  "dependencies": {
    "@types/doctrine": "^0.0.3",
    "babel-core": "^6.23.1",
    "bluebird-lst": "^1.0.1",
    "chalk": "^1.1.3",
    "doctrine": "^2.0.0",
    "fs-extra-p": "^4.0.2",
    "markdown-it": "^8.3.0",
    "source-map-support": "^0.4.11"
  },
  "peerDependencies": {
    "typescript": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts-babel.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "babel",
    "typescript"
  ],
  "devDependencies": {
    "@types/node": "^7.0.5",
    "jest": "^19.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test"
    ],
    "testPathIgnorePatterns": [
      "[\\/]{1}fixtures[\\/]{1}"
    ],
    "testRegex": "\\.js$"
  }
}
